{
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["ESNext"],
		"module": "commonjs",

		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,

		"strict": true,
		"noImplicitAny": true,
		"skipLibCheck": true,

		"rootDir": "./src",
		"outDir": "lib",
		"declaration": true
	}
}
